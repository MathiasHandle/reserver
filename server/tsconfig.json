{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./src",
    //"target": "ESNext",
    //"lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["src/**/*"]
}
